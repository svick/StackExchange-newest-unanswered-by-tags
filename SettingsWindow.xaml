<Window x:Class="Newest_unaswered_by_tags.SettingsWindow"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Settings" SizeToContent="Height" Loaded="Window_Loaded" Width="250">
	<Grid>
		<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />
		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>
			<RowDefinition />
			<RowDefinition />
		</Grid.RowDefinitions>
		<TextBox Style="{StaticResource margin}" Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="2" Name="tagsTextBox" />
		<Button Grid.Column="0" Grid.Row="1" Style="{StaticResource margin}" Click="Ok_Click">OK</Button>
		<Button Grid.Column="1" Grid.Row="1" Style="{StaticResource margin}" Click="Cancel_Click">Cancel</Button>
	</Grid>
</Window>
