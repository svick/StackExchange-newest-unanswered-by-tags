<Application x:Class="Newest_unaswered_by_tags.App"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	StartupUri="MainWindow.xaml" DispatcherUnhandledException="Application_DispatcherUnhandledException">
	<Application.Resources>
		<Style TargetType="Hyperlink">
			<EventSetter Event="RequestNavigate" Handler="Hyperlink_RequestNavigate" />
		</Style>
		<Style TargetType="FrameworkElement" x:Key="margin">
			<Setter Property="Margin" Value="5" />
		</Style>
		<Style TargetType="FrameworkElement" x:Key="gridContent">
			<Setter Property="VerticalAlignment" Value="Center" />
			<Setter Property="Margin" Value="3" />
		</Style>
		<Style TargetType="FrameworkElement" x:Key="gridContentCenter" BasedOn="{StaticResource gridContent}">
			<Setter Property="HorizontalAlignment" Value="Center" />
		</Style>
		<Style TargetType="DataGridCell" x:Key="cellStyle">
			<Setter Property="BorderThickness" Value="0" />
		</Style>
	</Application.Resources>
</Application>
